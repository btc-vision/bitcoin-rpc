{
    "type": "module",
    "name": "@btc-vision/bsi-bitcoin-rpc",
    "version": "1.0.26",
    "description": "The one and only fully typed Bitcoin RPC client for Node.js",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "typings": "build/index.d.ts",
    "exports": {
        ".": {
            "import": "./build/index.js",
            "require": "./cjs/index.js",
            "types": "./src/index.ts"
        }
    },
    "scripts": {
        "watch": "gulp watch",
        "start": "cd build && node index",
        "build": "gulp && gulp cjs"
    },
    "publishConfig": {
        "access": "public",
        "tag": "latest"
    },
    "author": "BlobMaster41",
    "license": "MIT",
    "devDependencies": {
        "@babel/preset-env": "^7.24.3",
        "@types/node": "^20.11.30",
        "eslint": "^8.57.0",
        "prettier": "3.2.5",
        "typescript": "^5.4.3",
        "typescript-eslint": "^7.4.0"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/btc-vision/bsi-bitcoin-rpc.git"
    },
    "homepage": "https://opnet.org",
    "keywords": [
        "bitcoin",
        "opnet",
        "btc",
        "bitcoin smart contracts",
        "smart inscriptions",
        "bsi",
        "bsi-binary",
        "bsi-bitcoin-rpc",
        "ordinals"
    ],
    "dependencies": {
        "@btc-vision/bsi-common": "^1.0.20",
        "gulp": "^4.0.2",
        "gulp-cached": "^1.1.1",
        "gulp-logger": "^0.0.2",
        "gulp-typescript": "^6.0.0-alpha.1",
        "gulplog": "^2.2.0",
        "rpc-request": "^7.1.11",
        "ts-node": "^10.9.2",
        "undici": "^6.19.2"
    }
}
